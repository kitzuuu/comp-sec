generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int     @id @default(autoincrement())
  username String  @unique
  password String
  balance  Int
  blocked  Boolean
  admin    Boolean @default(false) // This is the new column
}
